  langid = {english},
  file = {/Users/dasirianni/Zotero/storage/8GZW2ZEL/Vereecken et al. - 2018 - Perspective on Mechanism Development and Structure.pdf;/Users/dasirianni/Zotero/storage/G3QDRM6R/kin.html}
}

@article{vermeire:2022:1304,
  title = {Detailed {{Kinetic Modeling}} for the {{Pyrolysis}} of a {{Jet A Surrogate}}},
  author = {Vermeire, Florence H. and Aravindakshan, Syam Ukkandath and Jocher, Agnes and Liu, Mengjie and Chu, Te-Chun and Hawtof, Ryan E. and {Van de Vijver}, Ruben and Prendergast, Matthew B. and Van Geem, Kevin M. and Green, William H.},
