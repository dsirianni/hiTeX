@article{carlson1993,
  title = {Accuracy of Free Energies of Hydration for Organic Molecules from 6-31g*-Derived Partial Charges},
  author = {Carlson, Heather A. and Nguyen, Toan B. and Orozco, Modesto and Jorgensen, William L.},
  year = {1993},
  journal = {Journal of Computational Chemistry},
  volume = {14},
  number = {10},
  pages = {1240--1249},
  issn = {1096-987X},
  doi = {10.1002/jcc.540141013},
  urldate = {2022-03-09},
  abstract = {Absolute free energies of hydration have been computed for 13 diverse organic molecules using partial charges derived from ab initio 6-31G* wave functions. Both Mulliken charges and charges fit to the electrostatic potential surface (EPS) were considered in conjunction with OPLS Lennard\textendash Jones parameters for the organic molecules and the TIP4P model of water. Monte Carlo simulations with statistical perturbation theory yielded relative free energies of hydration. These were converted to absolute quantities through perturbations to reference molecules for which absolute free energies of hydration had been obtained previously in TIP4P water. The average errors in the computed absolute free energies of hydration are 1.1 kcal/mol for the 6-31G* EPS charges and 4.0 kcal/mol for the Mulliken charges. For the EPS charges, the largest individual errors are under 2 kcal/mol except for acetamide, in which case the error is 3.7 kcal/mol. The hydrogen bonding between the organic solutes and water has also been characterized. \textcopyright{} John Wiley \& Sons, Inc.},
  langid = {english},
  file = {../../../Zotero/storage/UQ8Y6I63/jcc.html}
}

@article{casanova2008,
  title = {The Spin-Flip Extended Single Excitation Configuration Interaction Method},
  author = {Casanova, David and {Head-Gordon}, Martin},
  year = {2008},
  month = aug,
  journal = {The Journal of Chemical Physics},
  volume = {129},
  number = {6},
  pages = {064104},
  publisher = {{American Institute of Physics}},
  issn = {0021-9606},
  doi = {10.1063/1.2965131},
  urldate = {2022-06-09},
  abstract = {An extension of the spin-flip single excitation configuration interaction (SF-CIS) method is introduced. The extension, abbreviated as SF-XCIS, includes all configurations in which no more than one virtual level of the high spin triplet reference becomes occupied and no more than one doubly occupied level becomes vacant. The number of such configurations is quadratic with molecule size, and the method is implemented in a direct algorithm whose cost scales in the same way with molecule size as CIS itself, thus permitting applications to large systems. Starting from a spin restricted triplet determinant, SF-XCIS yields spin-pure singlet, triplet, and quintet states, and treats both half-occupied reference orbitals in a fully balanced way to allow application to strongly correlated problems. Tests on bond dissociation in the HF molecule, the torsional potential of ethylene, and excited states of polyenes show encouraging improvements using SF-XCIS compared to SF-CIS and a previously suggested extension, the spin-complete CIS model.}
}

