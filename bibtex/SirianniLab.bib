@article{wang:2018:502,
  title = {A Physics-Based Approach to Modeling Real-Fuel Combustion Chemistry - {{I}}. {{Evidence}} from Experiments, and Thermodynamic, Chemical Kinetic and Statistical Considerations},
  author = {Wang, Hai and Xu, Rui and Wang, Kun and Bowman, Craig T. and Hanson, Ronald K. and Davidson, David F. and Brezinsky, Kenneth and Egolfopoulos, Fokion N.},
  year = {2018},
