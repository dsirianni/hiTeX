@article{michalik2023,
  title = {Magnetic Fraction of the Atmospheric Dust in {{Krak\'ow}} \textendash{} Physicochemical Characteristics and Possible Environmental Impact},
  author = {Michalik, Jan M. and {Wilczy{\'n}ska-Michalik}, Wanda and Gondek, {\L}ukasz and Tokarz, Waldemar and {\.Z}ukrowski, Jan and Gajewska, Marta and Michalik, Marek},
