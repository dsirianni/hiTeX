  author = {Heald, C. L. and Goldstein, A. H. and Allan, J. D. and Aiken, A. C. and Apel, E. and Atlas, E. L. and Baker, A. K. and Bates, T. S. and Beyersdorf, A. J. and Blake, D. R. and Campos, T. and Coe, H. and Crounse, J. D. and DeCarlo, P. F. and {de Gouw}, J. A. and Dunlea, E. J. and Flocke, F. M. and Fried, A. and Goldan, P. and Griffin, R. J. and Herndon, S. C. and Holloway, J. S. and Holzinger, R. and Jimenez, J. L. and Junkermann, W. and Kuster, W. C. and Lewis, A. C. and Meinardi, S. and Millet, D. B. and Onasch, T. and Polidori, A. and Quinn, P. K. and Riemer, D. D. and Roberts, J. M. and Salcedo, D. and Sive, B. and Swanson, A. L. and Talbot, R. and Warneke, C. and Weber, R. J. and Weibring, P. and Wennberg, P. O. and Worsnop, D. R. and Wittig, A. E. and Zhang, R. and Zheng, J. and Zheng, W.},
  year = {2008},
