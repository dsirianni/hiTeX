@article{brunken2019,
  title = {Gas-{{Phase Vibrational Spectroscopy}} of the {{Hydrocarbon Cations}} l-{{C3H}}+, {{HC3H}}+, and c-{{C3H2}}+: {{Structures}}, {{Isomers}}, and the {{Influence}} of {{Ne-Tagging}}},
  shorttitle = {Gas-{{Phase Vibrational Spectroscopy}} of the {{Hydrocarbon Cations}} l-{{C3H}}+, {{HC3H}}+, and c-{{C3H2}}+},
  author = {Br{\"u}nken, Sandra and Lipparini, Filippo and Stoffels, Alexander and Jusko, Pavol and Redlich, Britta and Gauss, J{\"u}rgen and Schlemmer, Stephan},
