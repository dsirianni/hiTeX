@article{riu:2021:6151,
  title = {Understanding the {{Nature}} and {{Properties}} of {{Hydrogen}}\textendash{{Hydrogen Bonds}}: {{The Stability}} of a {{Bulky Phosphatetrahedrane}} as a {{Case Study}}},
  shorttitle = {Understanding the {{Nature}} and {{Properties}} of {{Hydrogen}}\textendash{{Hydrogen Bonds}}},
