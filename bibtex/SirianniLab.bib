  author = {{Addison-Smith}, Ian and Guzman, Horacio V. and Cooper, Christopher D.},
  year = {2023},
  month = may,
  journal = {Journal of Chemical Theory and Computation},
  volume = {19},
  number = {10},
  pages = {2996--3006},
  publisher = {{American Chemical Society}},
  issn = {1549-9618},
  doi = {10.1021/acs.jctc.3c00021},
  urldate = {2023-07-06},
  abstract = {An accurate force calculation with the Poisson\textendash Boltzmann equation is challenging, as it requires the electric field on the molecular surface. Here we present a calculation of the electric field on the solute\textendash solvent interface that is exact for piecewise linear variations of the potential and analyze four different alternatives to compute the force using a boundary element method. We performed a verification exercise for two cases: the isolated and two interacting molecules. Our results suggest that the boundary element method outperforms the finite difference method, as the latter needs a much finer mesh than in solvation energy calculations to get acceptable accuracy in the force, whereas the same surface mesh as in a standard energy calculation is appropriate for the boundary element method. Among the four evaluated alternatives of force calculation, we saw that the most accurate one is based on the Maxwell stress tensor. However, for a realistic application, like the barnase\textendash barstar complex, the approach based on variations of the energy functional, which is less accurate, gives equivalent results. This analysis is useful toward using the Poisson\textendash Boltzmann equation for force calculations in applications where high accuracy is key, for example, to feed molecular dynamics models or to enable the study of the interaction between large molecular structures, like viruses adsorbed onto substrates.},
  file = {../../../Zotero/storage/IJKB3YWS/Addison-Smith et al. - 2023 - Accurate Boundary Integral Formulations for the Ca.pdf}
}

