@article{hesselmann:2011:732,
  title = {On the Accuracy of {{DFT-SAPT}}, {{MP2}}, {{SCS-MP2}}, {{MP2C}}, and {{DFT}}+{{Disp}} Methods for the Interaction Energies of Endohedral Complexes of the {{C}} 60fullerene with a Rare Gas Atom},
  author = {He{\ss}elmann, Andreas and Korona, Tatiana},
  year = {2011},
  journal = {Phys. Chem. Chem. Phys.},
  volume = {13},
