  keywords = {Molecular dynamics,Oxy-fuel combustion,Reaction pathways,ReaxFF},
  file = {/Users/dasirianni/Zotero/storage/H9XTLZ6E/Qiu et al. - 2020 - Reactive force field molecular dynamics (ReaxFF MD.pdf;/Users/dasirianni/Zotero/storage/TTI7ZKUE/S0032591019305959.html}
