@article{fridlyand:2013:732,
  title = {N-{{Heptane Pyrolysis}} and {{Oxidation}} in {{Ethylene}}\textendash{{Methane}} and {{Iso-Octane Mixtures}}},
  author = {Fridlyand, Aleksandr and Brezinsky, Kenneth and Mandelbaum, Andrew},
  year = {2013},
  month = may,
  journal = {Journal of Propulsion and Power},
  volume = {29},
  number = {3},
  pages = {732--743},
  publisher = {{American Institute of Aeronautics and Astronautics}},
  issn = {0748-4658},
  doi = {10.2514/1.B34875},
  urldate = {2023-08-28},
  abstract = {Single pulse shock-tube experiments were conducted examining how ignition delay may be influenced by the interactive chemistry of fuels and their pyrolytic and oxidative decomposition products. Ignition delay was not measured directly but is assumed to be positively correlated to the temperature (above 1000~K) at which  CO 2 CO2  is formed in constant time experiments. Using this definition, it was found that heptane, ethylene, heptane\textendash ethylene, and heptane\textendash ethylene\textendash methane mixtures had similar  CO 2 CO2  formation temperatures. This suggests that the primary reasons for the difference in ignition delay of the individual constituents and their mixtures are the reactions of the parent fuel leading to formation of the smaller molecules and the thermochemistry of the system. In contrast, heptane\textendash iso-octane mixtures had different  CO 2 CO2  onset temperatures compared with pure components, suggesting that the cross reactions between radical pools of both components affect the overall chemistry. Heptane and iso-octane, pure and in mixtures, would then be expected to have different ignition delays, as has been observed. Simulating the experimental results using models available in the literature led to large variability of predictions. None of the models were deemed sufficient to reveal the chemical pathways responsible for the observed results.},
  keywords = {Combustors,Fuel Consumption,Gasoline,Pyrolysis,Reaction Rate Constant,Shock Tube,Supersonic Combustion Ramjet,Surrogate Model,Thermal Decomposition,Thermochemistry},
  file = {/Users/dasirianni/Zotero/storage/XZQ8NE6A/Fridlyand et al. - 2013 - N-Heptane Pyrolysis and Oxidation in Ethylene–Meth.pdf}
}

