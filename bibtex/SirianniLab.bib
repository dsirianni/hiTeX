@article{barone2023,
  title = {Reliable {{Gas Phase Reaction Rates}} at {{Affordable Cost}} by {{Means}} of the {{Parameter-Free JunChS-F12 Model Chemistry}}},
  author = {Barone, Vincenzo and Lupi, Jacopo and Salta, Zoi and Tasinato, Nicola},
  year = {2023},
