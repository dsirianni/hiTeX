@article{heil:2011:3407,
  title = {Experimental Investigation on the Effect of {{O2}} and {{CO2}} on Burning Rates during Oxyfuel Combustion of Methane},
  author = {Heil, P. and Toporov, D. and F{\"o}rster, M. and Kneer, R.},
  year = {2011},
  journal = {Proceedings of the Combustion Institute},
  volume = {33},
  number = {2},
  pages = {3407--3413},
  issn = {15407489},
  doi = {10.1016/j.proci.2010.05.047},
  urldate = {2023-08-18},
  abstract = {For combustion in an CO2/O2 atmosphere, known as oxy-combustion, altered combustion rates have to be expected. Previous investigations reported that this can be explained not only with the different thermophysical and radiative properties, but also by the fact that CO2 participates directly in the chemical reactions. This paper presents an experimental study on oxyfuel methane combustion with the aim to investigate the importance of the chemical effects of high CO2 concentrations. Experiments have been carried out in a 25 kW furnace for flameless combustion which provides the possibilities to achieve stable combustion of methane within a wide range of oxygen concentrations in the CO2/O2 mixture at constant reactor temperature. This allows to focus on the chemical effects of CO2 by keeping the remaining factors affecting the combustion rate constant. Four different oxidizer mixtures (CO2/O2 and N2/O2 both with 21 vol\% and 18 vol\% O2) have been studied by detailed in-furnace measurements for flue gas compositions and temperature. In case of combustion in N2/O2 atmosphere, the CO profiles obtained for different O2 concentrations overlap thus demonstrating that changing the O2 concentration did not affect combustion rates, with keeping the temperature constant. In case of combustion in CO2/O2 atmosphere, the CO concentrations obtained were much higher than those in N2/O2 atmosphere. In contrast to N2/O2, the O2 concentrations had a significant impact on the production and consumption rates of CO in oxyfuel combustion. The results obtained in this work demonstrated that by elimination of the influence of: molar heat capacity, CO2 dissociation, and thermal radiation, it can be estimated that the observed effects of high CO2 concentrations on combustion rates can be attributed to its participation in the chemical reactions. An increase of O2 in oxyfuel led to a reduction of this impact, however, further investigations on the exact mechanism are necessary.},
  langid = {english},
  file = {/Users/dasirianni/Zotero/storage/ZCZHZ95S/Heil et al. - 2011 - Experimental investigation on the effect of O2 and.pdf}
}

