@incollection{speight:2020:597,
  title = {Chapter 14 - {{Monomers}}, Polymers, and Plastics},
  booktitle = {Handbook of {{Industrial Hydrocarbon Processes}} ({{Second Edition}})},
  author = {Speight, James G.},
  editor = {Speight, James G.},
  year = {2020},
