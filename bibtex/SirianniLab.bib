@article{wang2019b,
  title = {Molecular {{Dynamics}} of {{Combustion Reactions}} in {{Supercritical Carbon Dioxide}}. 6. {{Computational Kinetics}} of {{Reactions}} between {{Hydrogen Atom}} and {{Oxygen Molecule H}} + {{O2}} {$\rightleftharpoons$} {{HO}} + {{O}} and {{H}} + {{O2}} {$\rightleftharpoons$} {{HO2}}},
  author = {Wang, Chun-Hung and Masunov, Art{\"e}m E. and Allison, Timothy C. and Chang, Sungho and Lim, Chansun and Jin, Yuin and Vasu, Subith S.},
  year = {2019},
