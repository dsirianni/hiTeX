@article{isaacman-vanwertz:2018:462,
  title = {Chemical Evolution of Atmospheric Organic Carbon over Multiple Generations of Oxidation},
  author = {{Isaacman-VanWertz}, Gabriel and Massoli, Paola and O'Brien, Rachel and Lim, Christopher and Franklin, Jonathan P. and Moss, Joshua A. and Hunter, James F. and Nowak, John B. and Canagaratna, Manjula R. and Misztal, Pawel K. and Arata, Caleb and Roscioli, Joseph R. and Herndon, Scott T. and Onasch, Timothy B. and Lambe, Andrew T. and Jayne, John T. and Su, Luping and Knopf, Daniel A. and Goldstein, Allen H. and Worsnop, Douglas R. and Kroll, Jesse H.},
