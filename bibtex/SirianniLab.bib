@article{murayama2023,
  title = {Characterizing {{Reaction Route Map}} of {{Realistic Molecular Reactions Based}} on {{Weight Rank Clique Filtration}} of {{Persistent Homology}}},
  author = {Murayama, Burai and Kobayashi, Masato and Aoki, Masamitsu and Ishibashi, Suguru and Saito, Takuya and Nakamura, Takenobu and Teramoto, Hiroshi and Taketsugu, Tetsuya},
