@article{panteleev:2018:897,
  title = {Molecular {{Dynamics Study}} of {{Combustion Reactions}} in a {{Supercritical Environment}}. {{Part}} 2: {{Boxed MD Study}} of {{CO}} + {{OH}} \textrightarrow{} {{CO2}} + {{H Reaction Kinetics}}},
  shorttitle = {Molecular {{Dynamics Study}} of {{Combustion Reactions}} in a {{Supercritical Environment}}. {{Part}} 2},
